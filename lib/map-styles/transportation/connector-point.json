{
  "id": "connector-point",
  "type": "circle",
  "source": "transportation",
  "source-layer": "connector",
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Point"
  ],
  "paint": {
    "circle-color": "$transportation.connector.color",
    "circle-radius": [
      "interpolate",
      [
        "exponential",
        2
      ],
      [
        "zoom"
      ],
      0,
      1,
      17,
      5
    ],
    "circle-opacity": 0.4
  },
  "layout": {},
  "metadata": {
    "overture:theme": "transportation",
    "overture:type": "connector",
    "overture:pass": "geometry",
    "overture:color": "$transportation.connector.color",
    "overture:activeColor": "$transportation.connector.activeColor"
  }
}
