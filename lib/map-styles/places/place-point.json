{
  "id": "place-point",
  "type": "circle",
  "source": "places",
  "source-layer": "place",
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Point"
  ],
  "minzoom": 11,
  "paint": {
    "circle-color": "$places.place.color",
    "circle-radius": [
      "interpolate",
      [
        "exponential",
        2
      ],
      [
        "zoom"
      ],
      0,
      1,
      17,
      5
    ],
    "circle-opacity": 0.4
  },
  "layout": {},
  "metadata": {
    "overture:theme": "places",
    "overture:type": "place",
    "overture:pass": "geometry",
    "overture:color": "$places.place.color",
    "overture:activeColor": "$places.place.activeColor"
  }
}
