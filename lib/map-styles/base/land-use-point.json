{
  "id": "land-use-point",
  "type": "circle",
  "source": "base",
  "source-layer": "land_use",
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Point"
  ],
  "minzoom": 13,
  "paint": {
    "circle-color": "$base.land_use_line.color",
    "circle-radius": [
      "interpolate",
      [
        "exponential",
        2
      ],
      [
        "zoom"
      ],
      0,
      1,
      17,
      6
    ],
    "circle-opacity": 0.4
  },
  "layout": {},
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land_use",
    "overture:pass": "geometry",
    "overture:color": "$base.land_use_line.color",
    "overture:activeColor": "$base.land_use_line.activeColor",
    "overture:activeOnly": true
  }
}
