{
  "id": "land-use-point-label",
  "type": "symbol",
  "source": "base",
  "source-layer": "land_use",
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Point"
  ],
  "minzoom": 13,
  "paint": {
    "text-color": "$base.land_use.textColor",
    "text-halo-color": "$base.land_use.textHaloColor",
    "text-halo-width": 1
  },
  "layout": {
    "text-font": "$global.fonts.regular",
    "text-field": [
      "get",
      "@name"
    ],
    "text-size": "$global.fonts.sizeSm",
    "text-radial-offset": 0,
    "text-justify": "auto",
    "text-line-height": 1,
    "text-padding": 12,
    "text-max-width": 4
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land_use",
    "overture:pass": "labels",
    "overture:activeOnly": true
  }
}
