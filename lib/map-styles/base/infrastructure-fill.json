{
  "id": "infrastructure-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "infrastructure",
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Polygon"
  ],
  "paint": {
    "fill-color": "$base.infrastructure.color",
    "fill-opacity": 0.5,
    "fill-outline-color": "$base.infrastructure.outlineColor"
  },
  "layout": {},
  "metadata": {
    "overture:theme": "base",
    "overture:type": "infrastructure",
    "overture:pass": "geometry",
    "overture:color": "$base.infrastructure.color",
    "overture:activeColor": "$base.infrastructure.activeColor",
    "overture:activeOnly": true
  }
}
