{
  "id": "building-fill-labels",
  "type": "symbol",
  "source": "buildings",
  "source-layer": "building",
  "filter": [
    "all",
    [
      "==",
      [
        "geometry-type"
      ],
      "Polygon"
    ]
  ],
  "paint": {
    "text-color": "$buildings.building.textColor",
    "text-halo-color": "$buildings.building.textHaloColor",
    "text-halo-width": 1
  },
  "layout": {
    "text-font": "$global.fonts.regular",
    "text-field": [
      "get",
      "@name"
    ],
    "text-size": "$global.fonts.sizeMd",
    "symbol-placement": "point"
  },
  "metadata": {
    "overture:theme": "buildings",
    "overture:type": "building",
    "overture:pass": "labels"
  }
}
