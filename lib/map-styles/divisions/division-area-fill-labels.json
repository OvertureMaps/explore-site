{
  "id": "division-area-fill-labels",
  "type": "symbol",
  "source": "divisions",
  "source-layer": "division_area",
  "filter": [
    "all",
    [
      "==",
      [
        "geometry-type"
      ],
      "Polygon"
    ]
  ],
  "paint": {
    "text-color": "$divisions.division_area.textColor",
    "text-halo-color": "$divisions.division_area.textHaloColor",
    "text-halo-width": 1
  },
  "layout": {
    "text-font": "$global.fonts.regular",
    "text-field": [
      "get",
      "@name"
    ],
    "text-size": "$global.fonts.sizeMd",
    "symbol-placement": "point"
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division_area",
    "overture:pass": "labels"
  }
}
