{
  "id": "building-extrusion",
  "type": "fill-extrusion",
  "source": "buildings",
  "source-layer": "building",
  "minzoom": 14,
  "filter": [
    "all",
    [
      "==",
      [
        "geometry-type"
      ],
      "Polygon"
    ],
    [
      "!=",
      [
        "get",
        "has_parts"
      ],
      true
    ]
  ],
  "paint": {
    "fill-extrusion-color": "$buildings.building.color.fillExtrusion",
    "fill-extrusion-base": [
      "case",
      [
        ">=",
        [
          "get",
          "min_height"
        ],
        0
      ],
      [
        "get",
        "min_height"
      ],
      0
    ],
    "fill-extrusion-height": [
      "case",
      [
        "has",
        "height"
      ],
      [
        "get",
        "height"
      ],
      0
    ],
    "fill-extrusion-opacity": [
      "interpolate",
      [
        "linear"
      ],
      [
        "zoom"
      ],
      15,
      0.4,
      16,
      0.63
    ]
  },
  "metadata": {
    "overture:theme": "buildings",
    "overture:type": "building",
    "overture:group": "buildings",
    "overture:selectable": true,
    "overture:item": "building-3d"
  }
}
