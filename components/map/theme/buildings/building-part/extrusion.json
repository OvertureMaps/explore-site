{
  "id": "building-part-extrusion",
  "type": "fill-extrusion",
  "source": "buildings",
  "source-layer": "building_part",
  "minzoom": 14,
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Polygon"
  ],
  "paint": {
    "fill-extrusion-color": "$buildings.building_part.color.fillExtrusion",
    "fill-extrusion-base": [
      "case",
      [
        ">=",
        [
          "get",
          "min_height"
        ],
        0
      ],
      [
        "get",
        "min_height"
      ],
      0
    ],
    "fill-extrusion-height": [
      "case",
      [
        "has",
        "height"
      ],
      [
        "get",
        "height"
      ],
      0
    ],
    "fill-extrusion-opacity": [
      "interpolate",
      [
        "linear"
      ],
      [
        "zoom"
      ],
      15,
      0.4,
      16,
      0.63
    ]
  },
  "metadata": {
    "overture:theme": "buildings",
    "overture:type": "building_part",
    "overture:group": "buildings",
    "overture:selectable": true,
    "overture:item": "building-part-3d"
  }
}
