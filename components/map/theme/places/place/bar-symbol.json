{
  "id": "bar-symbol",
  "type": "symbol",
  "source": "places",
  "source-layer": "place",
  "minzoom": 14,
  "filter": [
    "all",
    ["==", ["geometry-type"], "Point"],
    [">=", ["get", "confidence"], ["step", ["zoom"], 0.99, 15, 0.97, 16, 0.95, 17, 0.90]],
    ["match", "$extract:categories.primary",
      ["beach_bar", "beer_bar", "champagne_bar", "cocktail_bar", "dive_bar", "drive_thru_bar", "hotel_bar", "pub", "sake_bar", "lounge", "speakeasy", "sports_bar", "tiki_bar", "vermouth_bar", "whiskey_bar", "wine_bar"], true, false
    ]
  ],
  "layout": {
    "text-field": ["get", "@name"],
    "text-justify": "auto",
    "text-line-height": 1,
    "text-max-width": 6,
    "text-radial-offset": 1.1,
    "text-size": 12,
    "text-variable-anchor": ["literal", ["right", "left", "top", "bottom"]],
    "text-font": [
      "$globals.font.noto-sans-semicondensed.regular"
    ],
    "icon-image": "beer",
    "icon-allow-overlap": false,
    "icon-padding": 2,
    "icon-size": 0.75
  },
  "metadata": {
    "overture:theme": "places",
    "overture:type": "place",
    "overture:group": "places-categories",
    "overture:selectable": true,
    "overture:item": "bar"
  }
}
