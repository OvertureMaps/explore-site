{
  "id": "place-all-circle",
  "type": "symbol",
  "source": "places",
  "source-layer": "place",
  "minzoom": 10,
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Point"
  ],
  "layout": {
    "text-field": [
      "get",
      "@name"
    ],
    "text-justify": "auto",
    "text-line-height": 1,
    "text-max-width": 6,
    "text-radial-offset": 1,
    "text-size": 9,
    "text-variable-anchor": [
      "literal",
      [
        "right",
        "left",
        "top",
        "bottom"
      ]
    ],
    "text-font": [
      "$globals.font.noto-sans-semicondensed.regular"
    ],
    "icon-image": "circle",
    "icon-size": [
      "interpolate",
      [
        "linear"
      ],
      [
        "get",
        "confidence"
      ],
      0,
      0.2,
      0.5,
      0.2,
      0.7,
      0.3,
      0.9,
      0.4,
      1,
      0.5
    ],
    "icon-allow-overlap": false,
    "icon-padding": 4
  },
  "paint": {
    "text-color": "$places.place.color.text",
    "text-halo-color": "$places.place.color.textHalo",
    "text-halo-width": 1.5
  },
  "metadata": {
    "overture:theme": "places",
    "overture:type": "place",
    "overture:group": "places",
    "overture:selectable": true,
    "overture:item": "place-all-circle"
  }
}
