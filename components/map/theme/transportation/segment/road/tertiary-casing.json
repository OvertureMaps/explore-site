{
  "id": "segment-road-tertiary-casing",
  "type": "line",
  "source": "transportation",
  "source-layer": "segment",
  "minzoom": 12,
  "filter": [
    "all",
    ["==", ["get", "subtype"], "road"],
    ["==", ["get", "class"], "tertiary"]
  ],
  "paint": {
    "line-color": ["step", ["zoom"], "hsla(0, 0%, 0%, 0)", 14, "$transportation.road.color.casing"],
    "line-width": ["interpolate", ["exponential", 1.5], ["zoom"], 14, 0.8, 20, 2],
    "line-gap-width": ["interpolate", ["exponential", 1.5], ["zoom"], 11, 0.2, 12, 1, 16, 4, 20, 8]
  },
  "layout": { "line-cap": "round", "line-join": "round" },
  "metadata": {
    "overture:theme": "transportation",
    "overture:type": "segment",
    "overture:group": "transportation-roads",
    "overture:selectable": false,
    "overture:item": "road-tertiary"
  }
}
