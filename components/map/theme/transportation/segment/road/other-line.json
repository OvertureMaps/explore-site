{
  "id": "segment-road-other-line",
  "type": "line",
  "source": "transportation",
  "source-layer": "segment",
  "minzoom": 12,
  "filter": [
    "all",
    ["==", ["get", "subtype"], "road"],
    ["!", ["in", ["get", "class"], ["literal", ["motorway", "trunk", "primary", "secondary", "tertiary", "residential", "living_street", "pedestrian", "footway", "steps", "path", "track", "cycleway", "bridleway"]]]]
  ],
  "paint": {
    "line-color": "$transportation.road.color.roadFill",
    "line-width": [
      "interpolate",
      ["exponential", 1.5],
      ["zoom"],
      12, 0.2,
      16, 2,
      20, 4
    ]
  },
  "layout": {
    "line-cap": "round",
    "line-join": "round"
  },
  "metadata": {
    "overture:theme": "transportation",
    "overture:type": "segment",
    "overture:group": "transportation-roads",
    "overture:selectable": true,
    "overture:item": "road-other"
  }
}
