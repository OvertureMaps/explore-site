{
  "id": "land-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "land",
  "filter": [
    "all",
    ["==", ["geometry-type"], "Polygon"],
    [
      "step",["zoom"],
      ["!",["has","subtype"]],
      5,
      ["any",
        ["==",["get","class"],"land"],
        ["!",["has","subtype"]]
      ]
    ]
  ],
  "paint": {
    "fill-color": "$base.land.color.fill",
    "fill-outline-color": "$base.land.color.fillOutline"
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land",
    "overture:group": "base-land",
    "overture:selectable": true,
    "overture:item": "land"
  }
}
