{
  "type": "fill",
  "source": "base",
  "source-layer": "land",
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land",
    "overture:group": "base-land",
    "overture:item": "land-fill",
    "overture:selectable": true
  },
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Polygon"
  ],
  "paint": {
    "fill-color": "$inspect.land.color.fill",
    "fill-opacity": "$inspect.land.opacity"
  }
}
