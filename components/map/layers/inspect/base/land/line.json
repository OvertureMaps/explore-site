{
  "type": "line",
  "source": "base",
  "source-layer": "land",
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land",
    "overture:group": "base-land",
    "overture:item": "land-line",
    "overture:selectable": true
  },
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "LineString"
  ],
  "paint": {
    "line-color": "$inspect.land.color.line",
    "line-width": 1.5,
    "line-opacity": "$inspect.land.opacity"
  }
}
