{
  "type": "symbol",
  "source": "base",
  "source-layer": "water",
  "metadata": {
    "overture:theme": "base",
    "overture:type": "water",
    "overture:group": "base-water",
    "overture:item": "water-label",
    "overture:selectable": true
  },
  "minzoom": 10,
  "layout": {
    "text-field": [
      "get",
      "@name"
    ],
    "text-font": "$inspect.font",
    "text-size": 11,
    "text-max-width": 8,
    "text-allow-overlap": false
  },
  "paint": {
    "text-color": "$inspect.water.color.text",
    "text-halo-color": "$inspect.water.color.textHalo",
    "text-halo-width": 1,
    "text-opacity": "$inspect.water.opacity"
  }
}
