{
  "type": "symbol",
  "source": "transportation",
  "source-layer": "segment",
  "metadata": {
    "overture:theme": "transportation",
    "overture:type": "segment",
    "overture:group": "transportation-segment",
    "overture:item": "segment-label",
    "overture:selectable": true
  },
  "minzoom": 13,
  "layout": {
    "text-field": [
      "get",
      "@name"
    ],
    "text-font": "$inspect.font",
    "text-size": 11,
    "symbol-placement": "line",
    "text-allow-overlap": false
  },
  "paint": {
    "text-color": "$inspect.segment.color.text",
    "text-halo-color": "$inspect.segment.color.textHalo",
    "text-halo-width": 1,
    "text-opacity": "$inspect.segment.opacity"
  }
}
