{
  "id": "building-part-fill",
  "type": "fill",
  "source": "buildings",
  "source-layer": "building_part",
  "minzoom": 14,
  "filter": ["==", ["geometry-type"], "Polygon"],
  "paint": {
    "fill-color": "$buildings.building_part.color.fill",
    "fill-outline-color": "$buildings.building_part.color.fillOutline"
  },
  "metadata": {
    "overture:theme": "buildings",
    "overture:type": "building_part",
    "overture:group": "buildings",
    "overture:selectable": true,
    "overture:item": "building_part_footprint"
  }
}
