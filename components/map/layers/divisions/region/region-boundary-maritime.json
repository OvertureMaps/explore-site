{
  "id": "region-boundary-maritime",
  "type": "line",
  "source": "divisions",
  "source-layer": "division_boundary",
  "minzoom": 8,
  "filter": [
    "all",
    ["in", ["get", "subtype"], ["literal", ["region", "macroregion"]]],
    ["==", ["get", "class"], "maritime"]
  ],
  "paint": {
    "line-color": "$divisions.division_boundary.color.maritime",
    "line-width": ["interpolate", ["linear"], ["zoom"],
      8, 1.0,
      14, 1.5
    ],
    "line-opacity": 0.45,
    "line-dasharray": [3, 2]
  },
  "layout": {
    "line-cap": "butt"
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division_boundary",
    "overture:group": "divisions-region",
    "overture:selectable": true,
    "overture:item": "region_boundary"
  }
}
