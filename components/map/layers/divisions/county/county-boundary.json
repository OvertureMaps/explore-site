{
  "id": "county-boundary",
  "type": "line",
  "source": "divisions",
  "source-layer": "division_boundary",
  "filter": [
    "all",
    ["in", ["get", "subtype"], ["literal", ["macrocounty", "county"]]],
    ["step", ["zoom"], true, 8, ["!=", ["get", "class"], "maritime"]]
  ],
  "paint": {
    "line-color": ["match", ["get", "class"], "maritime", "$divisions.division_boundary.color.maritime", "$divisions.division_boundary.color.line"],
    "line-width": ["interpolate", ["linear"], ["zoom"],
      7, 0.5,
      9, 0.8,
      16, 1
    ],
    "line-opacity": 0.55,
    "line-dasharray": [1, 3]
  },
  "layout": {
    "line-cap": "round"
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division_boundary",
    "overture:group": "divisions-county",
    "overture:selectable": true,
    "overture:item": "county_boundary"
  }
}
