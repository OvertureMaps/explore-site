{
  "id": "division-label-highzoom",
  "type": "symbol",
  "source": "divisions",
  "source-layer": "division",
  "minzoom": 9,
  "filter": [
    "step",
    ["zoom"],
    [">=", ["coalesce", ["get", "population"], 0], 1000000],
    10,
    [">=", ["coalesce", ["get", "population"], 0], 100000],
    11,
    [">=", ["coalesce", ["get", "population"], 0], 10000],
    12,
    [">=", ["coalesce", ["get", "population"], 0], 1000],
    13,
    true
  ],
  "layout": {
    "text-field": ["get", "@name"],
    "text-font": ["$divisions.division.subtype.font.regular"],
    "text-size": [
      "interpolate",
      ["linear"],
      ["zoom"],
      9,
      [
        "interpolate",
        ["linear"],
        ["coalesce", ["get", "population"], 0],
        0, 7,
        10000, 8,
        100000, 10,
        1000000, 12,
        10000000, 14
      ],
      14,
      [
        "interpolate",
        ["linear"],
        ["coalesce", ["get", "population"], 0],
        0, 9,
        10000, 10,
        100000, 12,
        1000000, 15,
        10000000, 18
      ]
    ],
    "text-transform": "uppercase",
    "text-letter-spacing": 0.1,
    "text-max-width": 8,
    "text-allow-overlap": false,
    "symbol-sort-key": ["-", 0, ["coalesce", ["get", "population"], 0]]
  },
  "paint": {
    "text-color": "$divisions.division.subtype.color.text",
    "text-halo-color": "$divisions.division.subtype.color.textHalo",
    "text-halo-width": 1.5
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division",
    "overture:group": "divisions-labels",
    "overture:selectable": true,
    "overture:item": "division_label"
  }
}
