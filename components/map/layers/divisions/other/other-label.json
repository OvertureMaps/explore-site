{
  "id": "other-label",
  "type": "symbol",
  "source": "divisions",
  "source-layer": "division",
  "filter": ["all",
    ["match", ["get", "subtype"], ["localadmin", "borough", "macrohood", "neighborhood", "microhood"], true, false],
    ["step", ["zoom"],
      [">=", ["get", "population"], 15000],
      11, [">=", ["get", "population"], 5000],
      12, true
    ]
  ],
  "layout": {
    "text-field": ["get", "@name"],
    "text-font": ["match",
      ["get", "subtype"],["localadmin", "borough"], ["literal", "$divisions.division.fonts.medium"], ["literal", "$divisions.division.fonts.regular"]
    ],
    "text-size": [
      "interpolate", ["linear"], ["zoom"],
      10, ["match", ["get", "subtype"], ["localadmin", "borough"], 18, 14],
      13, ["match", ["get", "subtype"], ["localadmin", "borough"], 22, 18]
    ],
    "text-letter-spacing": 0.1,
    "text-max-width": 8,
    "text-allow-overlap": false
  },
  "paint": {
    "text-color": "$divisions.division.other.color.text",
    "text-halo-color": "$divisions.division.other.color.textHalo",
    "text-halo-width": 1.5
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division",
    "overture:group": "divisions-other",
    "overture:selectable": true,
    "overture:item": "other_label"
  }
}
