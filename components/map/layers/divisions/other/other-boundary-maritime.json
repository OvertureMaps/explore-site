{
  "id": "other-boundary-maritime",
  "type": "line",
  "source": "divisions",
  "source-layer": "division_boundary",
  "minzoom": 8,
  "filter": [
    "all",
    ["match", ["get", "subtype"], ["localadmin", "borough", "macrohood", "neighborhood", "microhood"], true, false],
    ["==", ["get", "class"], "maritime"]
  ],
  "paint": {
    "line-color": "$divisions.division_boundary.color.maritime",
    "line-width": ["interpolate", ["linear"], ["zoom"],
      8, 0.1,
      12, 0.2
    ],
    "line-opacity": 0.5,
    "line-dasharray": [3, 2]
  },
  "layout": {
    "line-cap": "butt",
    "line-join": "round"
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division_boundary",
    "overture:group": "divisions-other",
    "overture:selectable": true,
    "overture:item": "other_boundary"
  }
}
