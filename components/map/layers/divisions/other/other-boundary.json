{
  "id": "other-boundary",
  "type": "line",
  "source": "divisions",
  "source-layer": "division_boundary",
  "filter": [
    "all",
    ["match", ["get", "subtype"], ["localadmin", "borough", "macrohood", "neighborhood", "microhood"], true, false],
    ["step", ["zoom"], true, 8, ["!=", ["get", "class"], "maritime"]]
  ],
  "paint": {
    "line-color": ["match", ["get", "class"], "maritime", "$divisions.division_boundary.color.maritime", "$divisions.division_boundary.color.line"],
    "line-width": [
      "interpolate", ["linear"], ["zoom"],
      8, 0.1,
      12, 0.2
    ],
    "line-opacity": 0.5
  },
  "layout": {
    "line-cap": "round",
    "line-join": "round"
  },
  "metadata": {
    "overture:theme": "divisions",
    "overture:type": "division_boundary",
    "overture:group": "divisions-other",
    "overture:selectable": true,
    "overture:item": "other_boundary"
  }
}
