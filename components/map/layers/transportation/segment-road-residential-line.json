{
  "id": "segment-road-residential-line",
  "type": "line",
  "source": "transportation",
  "source-layer": "segment",
  "minzoom": 12,
  "filter": [
    "all",
    ["==", ["get", "subtype"], "road"],
    ["==", ["get", "class"], "residential"]
  ],
  "paint": {
    "line-color": "$transportation.road.color.roadFill",
    "line-width": [
      "interpolate",
      ["exponential", 1.5],
      ["zoom"],
      12, 0.2,
      14, 0.8,
      16, 3,
      20, 6
    ]
  },
  "layout": {
    "line-cap": "round",
    "line-join": "round"
  },
  "metadata": {
    "overture:theme": "transportation",
    "overture:type": "segment",
    "overture:group": "transportation-roads",
    "overture:selectable": true,
    "overture:item": "road-residential"
  }
}
