{
  "id": "grocery-symbol",
  "type": "symbol",
  "source": "places",
  "source-layer": "place",
  "minzoom": 10,
  "filter": [
    "all",
    ["==", ["geometry-type"], "Point"],
    [">=", ["get", "confidence"], 0.90],
    ["match", "$extract:categories.primary",
      ["grocery_store", "specialty_grocery_store", "asian_grocery_store", "ethical_grocery", "indian_grocery_store", "japanese_grocery_store", "korean_grocery_store", "kosher_grocery_store", "mexican_grocery_store", "organic_grocery_store", "russian_grocery_store"], true, false
    ]
  ],
  "layout": {
    "text-field": ["get", "@name"],
    "text-justify": "auto",
    "text-line-height": 1,
    "text-max-width": 6,
    "text-radial-offset": 1.1,
    "text-size": 12,
    "text-variable-anchor": ["literal", ["right", "left", "top", "bottom"]],
    "text-font": [
      "$globals.font.noto-sans-semicondensed-regular"
    ],
    "icon-image": "grocery",
    "icon-allow-overlap": false,
    "icon-padding": 2,
    "icon-size": 0.75
  },
  "metadata": {
    "overture:theme": "places",
    "overture:type": "place",
    "overture:group": "places-categories",
    "overture:selectable": true,
    "overture:item": "grocery"
  }
}
