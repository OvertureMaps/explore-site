{
  "id": "place-point",
  "type": "circle",
  "source": "places",
  "source-layer": "place",
  "metadata": {
    "overture:theme": "places",
    "overture:type": "place",
    "overture:group": "places",
    "overture:selectable": true,
    "overture:item": "place"
  },
  "maxzoom": 10,
  "filter": [
    "==",
    [
      "geometry-type"
    ],
    "Point"
  ],
  "paint": {
    "circle-color": "$places.place.color.circle",
    "circle-radius": ["interpolate",["linear"],["zoom"],
      0,0,
      3,0.2,
      10,2
    ],
    "circle-opacity": 0.2
  }
}
