{
  "id": "land-cover-crop-outline",
  "type": "line",
  "source": "base",
  "source-layer": "land_cover",
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land_cover",
    "overture:group": "base-land-cover",
    "overture:selectable": true,
    "overture:item": "crop"
  },
  "filter": [
    "==",
    [
      "get",
      "subtype"
    ],
    "crop"
  ],
  "paint": {
    "line-color": "$base.land_cover.color.fill.crop",
    "line-blur": 1,
    "line-width": 1.1
  }
}
