{
  "id": "infrastructure-pier-line",
  "type": "line",
  "source": "base",
  "source-layer": "infrastructure",
  "minzoom": 12,
  "filter": [
    "all",
    ["==", ["geometry-type"], "LineString"],
    ["==", ["get", "subtype"], "pier"]
  ],
  "paint": {
    "line-color": "$base.infrastructure.pier.color.line",
    "line-width": [
      "interpolate",
      ["exponential", 1.5],
      ["zoom"],
      12, 1,
      16, 3,
      20, 6
    ]
  },
  "layout": {
    "line-cap": "round",
    "line-join": "round"
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "infrastructure",
    "overture:group": "base-infrastructure",
    "overture:selectable": true,
    "overture:item": "pier"
  }
}
