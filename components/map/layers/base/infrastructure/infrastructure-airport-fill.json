{
  "id": "infrastructure-airport-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "infrastructure",
  "minzoom": 12,
  "filter": [
    "all",
    ["==", ["geometry-type"], "Polygon"],
    ["==", ["get", "subtype"], "airport"]
  ],
  "paint": {
    "fill-color": "$base.infrastructure.airport.color.fill",
    "fill-outline-color": "$base.infrastructure.airport.color.fillOutline"
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "infrastructure",
    "overture:group": "base-infrastructure",
    "overture:selectable": true,
    "overture:item": "airport_area"
  }
}
