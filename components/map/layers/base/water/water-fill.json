{
  "id": "water-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "water",
  "filter": [
    "all",
    ["==", ["geometry-type"], "Polygon"],
    ["match", ["get", "is_intermittent"], ["false"], false, true],
    ["step",["zoom"],
      ["has", "subtype"],
      5, ["match", ["get", "subtype"], ["ocean"], false, true]
    ]
  ],
  "paint": {
    "fill-color": "$base.water.lakes.color.fill",
    "fill-opacity": 1,
    "fill-outline-color": "$base.water.lakes.color.fillOutline"
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "water",
    "overture:group": "base-land",
    "overture:selectable": true,
    "overture:item": "lakes-rivers"
  }
}
