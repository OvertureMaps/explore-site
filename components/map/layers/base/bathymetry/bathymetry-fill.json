{
  "id": "bathymetry-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "bathymetry",
  "filter": [
    "all",
    ["==", ["geometry-type"], "Polygon"]
  ],
  "paint": {
    "fill-color": [
      "case",
      ["==", ["get", "depth"], 0], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 10], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 20], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 30], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 40], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 50], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 60], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 70], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 80], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 90], "$base.bathymetry.color.fill.g0",
      ["==", ["get", "depth"], 100], "$base.bathymetry.color.fill.g1",
      ["==", ["get", "depth"], 500], "$base.bathymetry.color.fill.g2",
      ["==", ["get", "depth"], 1000], "$base.bathymetry.color.fill.g3",
      ["==", ["get", "depth"], 1500], "$base.bathymetry.color.fill.g4",
      ["==", ["get", "depth"], 2000], "$base.bathymetry.color.fill.g5",
      ["==", ["get", "depth"], 2500], "$base.bathymetry.color.fill.g6",
      ["==", ["get", "depth"], 3000], "$base.bathymetry.color.fill.g7",
      "$base.bathymetry.color.fill.fallback"
    ],
    "fill-outline-color": "$base.bathymetry.color.fillOutline",
    "fill-opacity": 1
  },
  "layout": {},
  "metadata": {
    "overture:theme": "base",
    "overture:type": "bathymetry",
    "overture:group": "base-land",
    "overture:selectable": true,
    "overture:item": "bathymetry"
  }
}
