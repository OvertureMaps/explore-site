{
  "id": "land-use-cemetery-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "land_use",
  "minzoom": 12,
  "filter": [
    "all",
    ["==", ["geometry-type"], "Polygon"],
    ["==", ["get", "subtype"], "cemetery"]
  ],
  "paint": {
    "fill-color": "$base.land_use.cemetery.color.fill",
    "fill-outline-color": "$base.land_use.cemetery.color.fillOutline"
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land_use",
    "overture:group": "base-land-use",
    "overture:selectable": true,
    "overture:item": "cemetery"
  }
}
