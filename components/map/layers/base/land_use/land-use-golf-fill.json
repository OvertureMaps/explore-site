{
  "id": "land-use-golf-fill",
  "type": "fill",
  "source": "base",
  "source-layer": "land_use",
  "minzoom": 12,
  "metadata": {
    "overture:theme": "base",
    "overture:type": "land_use",
    "overture:group": "base-land-use",
    "overture:selectable": true,
    "overture:item": "golf"
  },
  "filter": [
    "all",
    [
      "==",
      [
        "geometry-type"
      ],
      "Polygon"
    ],
    [
      "==",
      [
        "get",
        "subtype"
      ],
      "golf"
    ]
  ],
  "paint": {
    "fill-color": "$base.land_use.golf.color.fill",
    "fill-outline-color": "$base.land_use.golf.color.fillOutline"
  }
}
