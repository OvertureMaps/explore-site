{
  "type": "symbol",
  "source": "places",
  "source-layer": "place",
  "minzoom": 14,
  "filter": [
    "all",
    [
      "==",
      [
        "geometry-type"
      ],
      "Point"
    ],
    [
      ">=",
      [
        "get",
        "confidence"
      ],
      [
        "step",
        [
          "zoom"
        ],
        0.99,
        15,
        0.97,
        16,
        0.95,
        17,
        0.9
      ]
    ],
    [
      ">=",
      [
        "index-of",
        "shop",
        "$extract:categories.primary"
      ],
      0
    ]
  ],
  "layout": {
    "text-field": [
      "get",
      "@name"
    ],
    "text-justify": "auto",
    "text-line-height": 1,
    "text-max-width": 6,
    "text-radial-offset": 1.1,
    "text-size": 12,
    "text-variable-anchor": [
      "literal",
      [
        "right",
        "left",
        "top",
        "bottom"
      ]
    ],
    "text-font": "$places.filtered",
    "icon-image": "shop",
    "icon-allow-overlap": false,
    "icon-padding": 2,
    "icon-size": 0.75
  },
  "metadata": {
    "overture:theme": "places",
    "overture:type": "place",
    "overture:group": "places-categories",
    "overture:selectable": true,
    "overture:item": "shop"
  }
}
