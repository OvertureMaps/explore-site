{
  "type": "fill",
  "source": "base",
  "source-layer": "bathymetry",
  "filter": [
    "all",
    [
      "==",
      [
        "geometry-type"
      ],
      "Polygon"
    ],
    [
      "==",
      [
        "get",
        "depth"
      ],
      3000
    ]
  ],
  "paint": {
    "fill-color": "$base.bathymetry.color.fill.depth.3000",
    "fill-outline-color": "$base.bathymetry.color.fillOutline",
    "fill-opacity": 1
  },
  "metadata": {
    "overture:theme": "base",
    "overture:type": "bathymetry",
    "overture:group": "base-bathymetry",
    "overture:selectable": true,
    "overture:item": "bathymetry-depth-3000"
  }
}
