{
  "type": "symbol",
  "source": "transportation",
  "source-layer": "segment",
  "minzoom": 13,
  "filter": [
    "all",
    [
      "==",
      [
        "get",
        "subtype"
      ],
      "road"
    ],
    [
      "==",
      [
        "get",
        "class"
      ],
      "secondary"
    ]
  ],
  "layout": {
    "text-field": [
      "get",
      "@name"
    ],
    "text-font": "$transportation.segment.road-secondary",
    "text-size": [
      "interpolate",
      [
        "linear"
      ],
      [
        "zoom"
      ],
      13,
      6,
      15,
      8,
      18,
      13
    ],
    "text-letter-spacing": 0.1,
    "text-max-angle": 30,
    "text-transform": "uppercase",
    "symbol-placement": "line",
    "symbol-spacing": [
      "interpolate",
      [
        "linear"
      ],
      [
        "zoom"
      ],
      12,
      150,
      18,
      450,
      22,
      1500
    ]
  },
  "paint": {
    "text-color": "$transportation.road.color.text",
    "text-halo-color": "$transportation.road.color.textHalo",
    "text-halo-width": 1
  },
  "metadata": {
    "overture:theme": "transportation",
    "overture:type": "segment",
    "overture:group": "transportation-roads",
    "overture:selectable": true,
    "overture:item": "road-secondary"
  }
}
