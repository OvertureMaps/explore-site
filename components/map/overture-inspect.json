{
  "base": {
    "land": { "color": { "fill": "$globals.color.inspect.land" } },
    "water": { "color": { "fill": "$globals.color.inspect.water" } },
    "bathymetry": { "color": { "fill": "$globals.color.inspect.bathymetry" } },
    "land_cover": { "color": { "fill": "$globals.color.inspect.landCover" } },
    "land_use": { "color": { "fill": "$globals.color.inspect.landUse" } }
  },
  "buildings": {
    "building": { "color": { "fill": "$globals.color.inspect.building" } },
    "building_part": { "color": { "fillExtrusion": "$globals.color.inspect.buildingPart" } }
  },
  "transportation": {
    "segment": { "color": { "line": "$globals.color.inspect.segment" } }
  },
  "addresses": {
    "address": { "color": { "circle": "$globals.color.inspect.address" } }
  },
  "places": {
    "place": { "color": { "circle": "$globals.color.inspect.place" } }
  },
  "divisions": {
    "division": { "color": { "text": "$globals.color.inspect.divisionPoint" } },
    "division_area": { "color": { "fill": "$globals.color.inspect.divisionFill" } },
    "division_boundary": { "color": { "line": "$globals.color.inspect.divisionLine" } }
  }
}
