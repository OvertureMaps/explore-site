{
  "addresses": {
    "address": {
      "display": {
        "name": "Addresses",
        "order": 1
      },
      "geometryType": "point",
      "color": {
        "fill": "$primitives.colors.address-base",
        "line": "$primitives.colors.address-base",
        "circle": "$primitives.colors.address-base"
      },
      "selection": {
        "fill": "$primitives.colors.address-active",
        "line": "$primitives.colors.address-active",
        "circle": "$primitives.colors.address-active"
      },
      "text": {
        "color": "$primitives.colors.address-text",
        "halo": "$primitives.colors.address-text-halo"
      }
    },
    "display": {
      "name": "Addresses",
      "order": 6
    }
  },
  "base": {
    "bathymetry": {
      "display": {
        "name": "Bathymetry",
        "order": 1
      },
      "geometryType": "polygon",
      "color": {
        "fill": "$primitives.colors.bathymetry-base",
        "fillOutline": "$primitives.colors.bathymetry-base",
        "line": "$primitives.colors.bathymetry-base",
        "circle": "$primitives.colors.bathymetry-base"
      },
      "selection": {
        "fill": "$primitives.colors.bathymetry-active",
        "line": "$primitives.colors.bathymetry-active",
        "circle": "$primitives.colors.bathymetry-active"
      },
      "text": {
        "color": "$primitives.colors.bathymetry-text",
        "halo": "$primitives.colors.bathymetry-text-halo"
      }
    },
    "display": {
      "name": "Base",
      "order": 1
    },
    "infrastructure": {
      "display": {
        "name": "Infrastructure",
        "order": 7
      },
      "geometryType": "line",
      "color": {
        "fill": "$primitives.colors.infrastructure-base",
        "fillOutline": "$primitives.colors.infrastructure-outline",
        "line": "$primitives.colors.infrastructure-base",
        "circle": "$primitives.colors.infrastructure-base"
      },
      "selection": {
        "fill": "$primitives.colors.infrastructure-active",
        "line": "$primitives.colors.infrastructure-active",
        "circle": "$primitives.colors.infrastructure-active"
      },
      "text": {
        "color": "$primitives.colors.infrastructure-text",
        "halo": "$primitives.colors.infrastructure-text-halo"
      }
    },
    "land": {
      "display": {
        "name": "Land",
        "order": 2
      },
      "geometryType": "polygon",
      "globeDefault": true,
      "color": {
        "fill": "$primitives.colors.land-warm-light",
        "fillOutline": "$primitives.colors.land-warm-outline",
        "line": "$primitives.colors.land-warm-light",
        "circle": "$primitives.colors.land-warm-light"
      },
      "selection": {
        "fill": "$primitives.colors.land-warm-active",
        "line": "$primitives.colors.land-warm-active",
        "circle": "$primitives.colors.land-warm-active"
      },
      "text": {
        "color": "$primitives.colors.land-warm-text",
        "halo": "$primitives.colors.land-warm-text-halo"
      }
    },
    "land_cover": {
      "display": {
        "name": "Land Cover",
        "order": 3,
        "subtypes": [
          {
            "name": "Moss",
            "order": 1,
            "color": "$primitives.colors.land-cover-moss"
          },
          {
            "name": "Forest",
            "order": 2,
            "color": "$primitives.colors.land-cover-forest"
          },
          {
            "name": "Grass",
            "order": 3,
            "color": "$primitives.colors.land-cover-grass"
          },
          {
            "name": "Crop",
            "order": 4,
            "color": "$primitives.colors.land-cover-crop"
          },
          {
            "name": "Shrub",
            "order": 5,
            "color": "$primitives.colors.land-cover-shrub"
          },
          {
            "name": "Barren",
            "order": 6,
            "color": "$primitives.colors.land-cover-barren"
          },
          {
            "name": "Wetland",
            "order": 7,
            "color": "$primitives.colors.land-cover-wetland"
          },
          {
            "name": "Mangrove",
            "order": 8,
            "color": "$primitives.colors.land-cover-mangrove"
          },
          {
            "name": "Urban",
            "order": 9,
            "color": "$primitives.colors.land-cover-urban"
          },
          {
            "name": "Snow",
            "order": 10,
            "color": "$primitives.colors.land-cover-snow"
          }
        ]
      },
      "geometryType": "polygon",
      "color": {
        "fill": [
          "match",
          [
            "get",
            "subtype"
          ],
          "moss",
          "$primitives.colors.land-cover-moss",
          "forest",
          "$primitives.colors.land-cover-forest",
          "grass",
          "$primitives.colors.land-cover-grass",
          "crop",
          "$primitives.colors.land-cover-crop",
          "shrub",
          "$primitives.colors.land-cover-shrub",
          "barren",
          "$primitives.colors.land-cover-barren",
          "wetland",
          "$primitives.colors.land-cover-wetland",
          "mangrove",
          "$primitives.colors.land-cover-mangrove",
          "urban",
          "$primitives.colors.land-cover-urban",
          "snow",
          "$primitives.colors.land-cover-snow",
          "$primitives.colors.transparent"
        ],
        "fillOutline": "$primitives.colors.land-cover-outline",
        "line": "$primitives.colors.transparent",
        "circle": "$primitives.colors.transparent"
      },
      "selection": {
        "fill": "$primitives.colors.land-cover-active",
        "line": "$primitives.colors.land-cover-active",
        "circle": "$primitives.colors.land-cover-active"
      },
      "text": {
        "color": "$primitives.colors.land-cover-text",
        "halo": "$primitives.colors.land-cover-text-halo"
      }
    },
    "land_use": {
      "display": {
        "name": "Land Use",
        "order": 4
      },
      "geometryType": "polygon",
      "color": {
        "fill": "$primitives.colors.land-use-fill",
        "fillOutline": "$primitives.colors.land-use-outline",
        "line": "$primitives.colors.land-use-fill",
        "circle": "$primitives.colors.land-use-fill"
      },
      "selection": {
        "fill": "$primitives.colors.land-use-active",
        "line": "$primitives.colors.land-use-active",
        "circle": "$primitives.colors.land-use-active"
      },
      "text": {
        "color": "$primitives.colors.land-use-text",
        "halo": "$primitives.colors.land-use-text-halo"
      }
    },
    "land_use_line": {
      "display": {
        "name": "Land Use Lines",
        "order": 5
      },
      "geometryType": "line",
      "color": {
        "fill": "$primitives.colors.land-use-line-base",
        "fillOutline": "$primitives.colors.land-use-line-base",
        "line": "$primitives.colors.land-use-line-base",
        "circle": "$primitives.colors.land-use-line-base"
      },
      "selection": {
        "fill": "$primitives.colors.land-use-line-active",
        "line": "$primitives.colors.land-use-line-active",
        "circle": "$primitives.colors.land-use-line-active"
      }
    },
    "water": {
      "display": {
        "name": "Water",
        "order": 6
      },
      "geometryType": "polygon",
      "globeDefault": true,
      "color": {
        "fill": "$primitives.colors.water-blue-light",
        "fillOutline": "$primitives.colors.water-blue-outline",
        "line": "$primitives.colors.water-blue-light",
        "circle": "$primitives.colors.water-blue-light"
      },
      "selection": {
        "fill": "$primitives.colors.water-blue-active",
        "line": "$primitives.colors.water-blue-active",
        "circle": "$primitives.colors.water-blue-active"
      },
      "text": {
        "color": "$primitives.colors.water-blue-text",
        "halo": "$primitives.colors.water-blue-text-halo"
      }
    }
  },
  "buildings": {
    "building": {
      "display": {
        "name": "Buildings",
        "order": 1
      },
      "geometryType": "polygon",
      "color": {
        "fill": "$primitives.colors.building-base",
        "fillExtrusion": "$primitives.colors.building-base",
        "line": "$primitives.colors.building-base",
        "circle": "$primitives.colors.building-base"
      },
      "selection": {
        "fill": "$primitives.colors.building-active",
        "fillExtrusion": "$primitives.colors.building-active",
        "line": "$primitives.colors.building-active",
        "circle": "$primitives.colors.building-active"
      },
      "text": {
        "color": "$primitives.colors.building-text",
        "halo": "$primitives.colors.building-text-halo"
      }
    },
    "building_part": {
      "display": {
        "name": "Building Parts",
        "order": 2
      },
      "geometryType": "polygon",
      "color": {
        "fill": "$primitives.colors.building-part-base",
        "fillExtrusion": "$primitives.colors.building-part-base",
        "line": "$primitives.colors.building-part-base",
        "circle": "$primitives.colors.building-part-base"
      },
      "selection": {
        "fill": "$primitives.colors.building-part-active",
        "fillExtrusion": "$primitives.colors.building-part-active",
        "line": "$primitives.colors.building-part-active",
        "circle": "$primitives.colors.building-part-active"
      },
      "text": {
        "color": "$primitives.colors.building-part-text",
        "halo": "$primitives.colors.building-part-text-halo"
      }
    },
    "display": {
      "name": "Buildings",
      "order": 3
    }
  },
  "divisions": {
    "display": {
      "name": "Divisions",
      "order": 2
    },
    "division": {
      "display": {
        "name": "Division Labels",
        "order": 3
      },
      "geometryType": "label",
      "textSize": [
        "interpolate",
        [
          "linear"
        ],
        [
          "zoom"
        ],
        2,
        10,
        10,
        14,
        12,
        12,
        16,
        16
      ]
    },
    "division_area": {
      "display": {
        "name": "Division Areas",
        "order": 1
      },
      "geometryType": "polygon",
      "color": {
        "fill": "$primitives.colors.division-area-base",
        "fillOutline": "$primitives.colors.division-area-outline",
        "line": "$primitives.colors.division-area-base",
        "circle": "$primitives.colors.division-area-base"
      },
      "selection": {
        "fill": "$primitives.colors.division-area-active",
        "line": "$primitives.colors.division-area-active",
        "circle": "$primitives.colors.division-area-active"
      },
      "text": {
        "color": "$primitives.colors.division-area-text",
