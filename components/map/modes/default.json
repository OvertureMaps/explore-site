      }
    },
    "division_boundary": {
      "display": {
        "name": "Division Boundaries",
        "order": 2
      },
      "geometryType": "line",
      "color": {
        "fill": "$primitives.colors.division-boundary-base",
        "line": "$primitives.colors.division-boundary-base",
        "circle": "$primitives.colors.division-boundary-base"
      },
      "selection": {
        "fill": "$primitives.colors.division-boundary-active",
        "line": "$primitives.colors.division-boundary-active",
        "circle": "$primitives.colors.division-boundary-active"
      },
      "text": {
        "color": "$primitives.colors.division-boundary-text",
        "halo": "$primitives.colors.division-boundary-text-halo"
      }
    }
  },
  "places": {
    "display": {
      "name": "Places",
      "order": 5
    },
    "place": {
      "display": {
        "name": "Places",
        "order": 1
      },
      "geometryType": "point",
      "color": {
        "fill": "$primitives.colors.place-base",
        "line": "$primitives.colors.place-base",
        "circle": "$primitives.colors.place-base"
      },
      "selection": {
        "fill": "$primitives.colors.place-active",
        "line": "$primitives.colors.place-active",
