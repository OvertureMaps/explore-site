      },
      "geometryType": "point",
      "color": {
        "fill": "$primitives.colors.connector-base",
        "line": "$primitives.colors.connector-base",
        "circle": "$primitives.colors.connector-base"
      },
      "selection": {
        "fill": "$primitives.colors.connector-active",
        "line": "$primitives.colors.connector-active",
